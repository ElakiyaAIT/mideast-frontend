    "@tinymce/tinymce-react": "^6.3.0",
    "axios": "^1.13.5",
    "clsx": "^2.1.1",
    "dompurify": "^3.3.1",
    "firebase": "^12.8.0",
    "i18next": "^25.7.4",
    "i18next-browser-languagedetector": "^8.2.0",
